{
  "name": "hackernews",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://moskowitza.github.io/hackernews",
  "dependencies": {
    "axios": "^0.19.0",
    "gh-pages": "^2.1.1",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-scripts": "5.0.1",
    "styled-components": "^4.4.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "^9.1.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js",
      "!src/serviceWorker.js",
      "!src/selectors/*.js",
      "!src/hooks/*.js",
      "!src/mapper/*.js",
      "!src/utils/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 95,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "coverageReporters": [
      "html",
      "text"
    ]
  }
}
